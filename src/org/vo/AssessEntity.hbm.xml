<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.vo.AssessEntity" table="assess" schema="ultimate">
        <id name="id">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="assess" column="assess"/>

        <many-to-one name="user" column="user_id" class="org.vo.UserRoleEntity"/>
        <many-to-one name="article" column="article_id" class="org.vo.ArticleEntity"/>
    </class>
</hibernate-mapping>